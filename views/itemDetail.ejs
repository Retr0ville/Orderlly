<!DOCTYPE html>
<html lang="en">
  <%-include ('./partials/head.ejs') %> 
<body id="body">
<%-include('./partials/header.ejs')  %> 
<div class="container">

  <div class="card-columns row row-cols-3">
    <div class="card col">
      <img class="card-img-top item" src="<%= item.img %>" alt="Card image cap">
      <div class="card-body item">
        <h5 class="card-title item-name"><%=item.itemName%></h5>
        <p class="card-text item-cost"><%= item.cost %></p>
        <p class="card-text item-description"><%= item.description %></p>
        <p class="card-text item-status"><%=item.itemStatus%></p>
      </div>
    </div>
  </div>

</div>
<%-include('./partials/footer.ejs') %>
</body>
</html>